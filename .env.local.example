# SUPABASE CONFIG
# Get these from your Supabase Project Settings -> API
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-secret-service-role-key

# Para Python (mismo que arriba)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-secret-service-role-key

# OPENAI CONFIG (REQUERIDO para AI)
# Get from https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-your_openai_api_key_here

# BINANCE CONFIG (V100 Live Bridge)
# Visit https://www.binance.com/en/my/settings/api-management
BINANCE_API_KEY=your_api_key_here
BINANCE_SECRET=your_secret_key_here
TRADING_MODE=PAPER # Set to LIVE to enable real Binance trading

# REDIS CONFIG (V1100 HA Layer)
# Railway will provide this automatically if you add Redis service
REDIS_URL=redis://default:password@redis.railway.internal:6379

# PUSHER CONFIG (Realtime V2 - REQUERIDO)
# Get from https://dashboard.pusher.com/
NEXT_PUBLIC_PUSHER_KEY=your_pusher_key
NEXT_PUBLIC_PUSHER_CLUSTER=mt1
PUSHER_APP_ID=your_pusher_app_id
PUSHER_KEY=your_pusher_key
PUSHER_SECRET=your_pusher_secret
PUSHER_CLUSTER=mt1

# OPTIONAL APIs
CMC_PRO_API_KEY=your_coinmarketcap_key
HELIUS_API_KEY=your_helius_key
BIRDEYE_API_KEY=your_birdeye_key
